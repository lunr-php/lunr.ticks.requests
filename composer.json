{
  "name": "lunr/ticks.requests",
  "description": "Lunr.Ticks.Requests is a hook for Requests that gathers analytics",
  "keywords": [ "requests", "analytics", "http", "curl" ],
  "homepage": "https://github.com/lunr-php/lunr.ticks.requests",
  "type": "library",
  "license": "MIT",
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/framna-nl-backend/requests.git"
    }
  ],
  "require": {
    "php": ">=8.1",
    "lunr/ticks": "dev-master",
    "rmccue/requests": "dev-release/2.0.x"
  },
  "require-dev": {
    "phpunit/phpunit": ">=10.5 <11.0",
    "lunr/halo": "dev-master",
    "mockery/mockery": "~1.6",
    "ext-xdebug": "~3.1"
  },
  "config": {
    "optimize-autoloader": true
  },
  "autoload": {
    "psr-4": {"Lunr\\": "src/Lunr/"}
  }
}
